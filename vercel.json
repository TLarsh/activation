{
    "builds": [
      {
        "src": "activation/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb", "runtime": "Python3.11.0" }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "activation/wsgi.py"
      }
    ]
}
